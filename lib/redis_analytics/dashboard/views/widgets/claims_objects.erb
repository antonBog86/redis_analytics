<table class="table table-bordered table-hover">
  <tr>
    <td>
      <p class="text-center"><b>Visits</b></p>
      <div id="claims_objects" style="width:100%;height:160px;"></div>
    </td>
  </tr>
</table>

<script type="text/javascript">
    claims_objects = Morris.Area({
        element: 'claims_objects',
        xkey: 'raw',
        ykeys: ['by_users', 'by_companies'],
        labels: ['Подано пользователями', 'Подано компаниями'],
        ymax: 'auto',
        grid: true,
        eventStrokeWidth: 2,
        eventLineColors: ['#ccc', '#eee'],
        smooth: true,
        hideHover: 'auto',
        parseTime: false,
        lineWidth: 1,
        pointSize: 0,
        xLabelFormat: function(x) {return x.getFullYear() + '-' + x.getMonth() + '-' + x.getDate();},
        pointFillColors: ['#fff'],
        pointStrokeColors: ['#000']
    });
</script>
